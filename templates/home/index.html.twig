                <img src="https://placehold.co/600x400?text=No+Image" class="img-fluid" alt="No Image">
            {% endif %}
            <div class="portfolio-info">
              <h4>{{ project.title }}</h4>
              <p>{{ project.category }}</p>
              <div class="portfolio-links">
                {% if project.image %}
                    <a href="{{ asset('assets/img/portfolio/' ~ project.image) }}" data-gallery="portfolioGallery" class="portfolio-lightbox" title="{{ project.title }}"><i class="bx bx-plus"></i></a>
                {% endif %}
                {% if project.projectUrl %}
                    <a href="{{ project.projectUrl }}" target="_blank" title="View Details"><i class="bx bx-link"></i></a>
                {% else %}
                    <a href="#" title="Details"><i class="bx bx-link"></i></a>
                {% endif %}
              </div>
            </div>
          </div>
        </div>
        {% else %}
            <div class="col-12 text-center">
                <p>No projects found.</p>
            </div>
        {% endfor %}

      </div>

    </div>
  </section><!-- End Portfolio Section -->

